{
  "name": "gatsby-typescript-starter",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby and TypeScript",
  "version": "1.0.0",
  "author": "Fool's Mate <mail@fools-mate.de>",
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@material-ui/core": "^4.9.12",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "@types/react-helmet": "^5.0.15",
    "emotion-normalize": "^10.1.0",
    "emotion-theming": "^10.0.27",
    "gatsby": "^2.19.43",
    "gatsby-image": "^2.2.43",
    "gatsby-plugin-emotion": "^4.3.0",
    "gatsby-plugin-manifest": "^2.2.47",
    "gatsby-plugin-offline": "^3.0.40",
    "gatsby-plugin-react-helmet": "^3.1.23",
    "gatsby-plugin-sharp": "^2.4.12",
    "gatsby-plugin-typescript": "^2.2.5",
    "gatsby-source-filesystem": "^2.1.55",
    "gatsby-transformer-sharp": "^2.3.18",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "prettier": "^1.19.1",
    "stylelint": "^15.10.1",
    "stylelint-config-recess-order": "^2.0.4",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^20.0.0"
  },
  "keywords": [
    "gatsby",
    "typescript",
    "emotion",
    "material-ui",
    "prettier",
    "stylelint"
  ],
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "fix:css": "stylelint --fix ./src/components/*.tsx"
  }
}
